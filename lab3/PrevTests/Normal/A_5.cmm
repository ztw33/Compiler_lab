int compare(int a, int b) {
	if (a > b) return a;
	if (a < b) return b;
	return 0;
}

int add(int aa, int bb) {
	return aa + bb;
}

int main() {
	int i[10], r[5], n, m;
	n = 0;
	while (n < 10) {
		i[n] = read();
		n = n + 1;
	}
	n = 0;
	m = 0;
	while(n < 10) {
		r[m] = compare(i[n], i[n+1]);
		n = n + 2; 
		m = m + 1;
	}
	n = 0;
	while (n < 5) {
		r[n] = add(r[n], i[n]);
		write(r[n]);
		n = n + 1;
	}
	return 0;
}
